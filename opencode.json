{
  "$schema": "https://opencode.ai/config.json",
  "mcp": {
    "cloudflare-docs": {
      "type": "local",
      "command": ["npx", "mcp-remote", "https://docs.mcp.cloudflare.com/sse"],
      "enabled": true
    },
    "cloudflare-bindings": {
      "type": "local",
      "command": [
        "npx",
        "mcp-remote",
        "https://bindings.mcp.cloudflare.com/sse"
      ],
      "enabled": true
    }
  }
}
