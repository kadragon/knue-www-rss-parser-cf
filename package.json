    "fast-xml-parser": "^4.5.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^22.10.2",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.6",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "wrangler": "^3.95.0"
  }
